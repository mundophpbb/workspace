{
    "name": "mundophpbb/workspace",
    "type": "phpbb-extension",
    "description": "phpBB Workspace IDE - A full-featured virtual IDE to manage projects, write code, and organize files directly within the forum panel.",
    "homepage": "https://github.com/mundophpbb/workspace",
    "version": "2.0.1-dev",
    "time": "2026-02-13",
    "license": "GPL-2.0-only",
    "authors": [
        {
            "name": "Chico Gois",
            "homepage": "https://mundophpbb.com.br",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.4",
        "composer/installers": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "mundophpbb\\workspace\\": "src/"
        }
    },
    "extra": {
        "display-name": "Mundo phpBB Workspace",
        "soft-require": {
            "phpbb/phpbb": ">=3.3.0"
        },
        "version-check": {
            "host": "github.com",
            "directory": "/mundophpbb/workspace/master",
            "filename": "version_check.json"
        }
    }

}

