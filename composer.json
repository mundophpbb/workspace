{
    "name": "mundophpbb/workspace",
    "type": "phpbb-extension",
    "description": "Mundo phpBB Workspace - Uma IDE virtual completa para gerenciar projetos, codificar e organizar arquivos diretamente no painel do forum.",
    "homepage": "https://github.com/mundophpbb/workspace",
    "version": "1.1.18-dev",
    "time": "2026-02-13",
    "license": "GPL-2.0-only",
    "authors": [
        {
            "name": "Chico Gois",
            "homepage": "https://mundophpbb.com.br",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.4",
        "composer/installers": "~1.0"
    },
    "extra": {
        "display-name": "Mundo phpBB Workspace",
        "soft-require": {
            "phpbb/phpbb": ">=3.3.0"
        },
        "version-check": {
            "host": "github.com",
            "directory": "/mundophpbb/workspace/master",
            "filename": "version_check.json"
        }
    }
}